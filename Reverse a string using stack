class Solution {
    
    public void reverse(String str){
       //your code here
       
       Stack<Character> s= new Stack<>();
       
       for(int i=0; i<str.length() ; i++) 
            s.push(str.charAt(i));
        
        String ans= "";
       while(s.size()>0){
           ans+= s.pop();
       }
       
       System.out.println(ans);
       
       
    }

}
