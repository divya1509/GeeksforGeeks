class GFG {
	public static void main (String[] args) {
		//code
		Scanner scn = new Scanner(System.in);
		int t= scn.nextInt();
		while(t>0){
		    int n = scn.nextInt();
		    int k = scn.nextInt();
		    int[] arr = new int[n];
		    for(int i=0; i<n; i++) arr[i]= scn.nextInt();
		    
		    Arrays.sort(arr);
		    int p= n-1;
		    for(int i=0; i<k; i++) {
		        System.out.print(arr[p]+ " ");
		        p--;
		        
		    }
		    
		    System.out.println();
		    t--;
		}
		
		
	}
}
