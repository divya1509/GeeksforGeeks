public void quadraticRoots(int a,int b,int c)
    {
        
        double d = b*b - 4*a*c;
        if(d<0){
            System.out.print("Imaginary");
            return;
        }
        //Your code here
        double r1= (double)((-b + Math.sqrt(d))/(2.0*a));
        double r2= (double)((-b - Math.sqrt(d))/(2.0*a));
        
        int a1= (int) (Math.floor(r1));
        int a2=(int)(Math.floor(r2));
        
        System.out.print(a1+ " " + a2);
    }
