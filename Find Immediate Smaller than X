public static int immediateSmaller(int arr[], int n, int x)
    {
        // Your code here
        int min = Integer.MIN_VALUE;
        for(int i=0; i<n; i++){
            if(arr[i]>min && arr[i]<x) min= arr[i];
        }
        
        if(min==Integer.MIN_VALUE) return -1;
        return min;
    }
